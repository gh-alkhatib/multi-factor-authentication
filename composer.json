{
    "name": "gh-alkhatib/multi-factor-authentication",
    "description": "A comprehensive multi-factor authentication package for Laravel applications",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "authentication",
        "mfa",
        "multi-factor",
        "2fa",
        "two-factor",
        "totp",
        "security"
    ],
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/auth": "^10.0|^11.0",
        "pragmarx/google2fa": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "MultiFactorAuthentication\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MultiFactorAuthentication\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MultiFactorAuthentication\\MultiFactorAuthenticationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
